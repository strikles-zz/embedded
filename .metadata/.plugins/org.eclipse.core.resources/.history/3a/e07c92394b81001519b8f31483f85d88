#ifndef SENSORS_H_
#define SENSORS_H_

// brightness
typedef enum {
	dark,
	average,
	bright,
	max_brightness_levels
}  eBrightness;

// sensors
typedef enum {
	battery,
	brightness,
	humidity,
	noise,
	personsPassed,
	temperature,
	max_sensors
} eSensor;

#endif /* SENSORS_H_ */
